<script>
  var connectType = "/login";
  
  window.addEventListener("load", async function () {
    setupPage(checkPage());
  })
  
function setupPage(values){
  document.getElementById("form").setAttribute("action",values.connect)
  
  values.form.inputs.forEach((e)=>{
    document.getElementById("formInputs").innerHTML += `<input type="${e.type || "text"}" name="${e.id}" placeholder="${e.name||"Null"}" class="simple-theme-input simple-theme-border simple-padding simple-round" style="width:100%" required>  <br><br>`;
    
  });
  values.form.buttons.forEach((e)=>{
    document.getElementById("formButtons").innerHTML +=  `<button class="simple-button simple-theme-button simple-round" onclick="${e.function||""}"><i class="fa fa-pencil"></i> ${e.name||"Null"}</button> `
  })
  connectType = values.connect;
  
  document.getElementById("title").innerHTML = values.title;
}
