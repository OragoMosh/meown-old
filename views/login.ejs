function checkPage(){
  var types = {
    "login":{
      "title":"Login",
      "connect":"/login",
      "form":{
        "inputs":[
          { id:"username", name:"Username", required:true },
          { id:"password", type:"password", name:"Password", required:true }
        ],
        "buttons":[
          { name:"Login" , function:"connect()"}
        ]
      }
      
    },
    "register":{
      "title":"Register",
      "connect":"/register",
      "form":{
        "inputs":[
          { id:"username", name:"Username", required:true },
          { id:"description", name:"Description", required:true },
          { id:"password", type:"password", name:"Password", required:true },
          { id:"passwordconfirm", type:"password", name:"Confirm Password", required:true },
          { id:"email", name:"Email", required:true }
        ],
        "buttons":[
          { name:"Register!" , function:"connect()"}
        ]
      }
    }
  }
  var result;
  
  if(getUrl(window.location.href).pathname){
    var i = getUrl(window.location.href).pathname;
    
    if (i == "/login") result = types.login;
    if (i == "/register") result = types.register;
  }
  return result;
}
</script>
