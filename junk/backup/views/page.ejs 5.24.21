<script>
  var me = getCookie("saved-username")||"guest";
  var id = "<%- values.id %>";
  var category = "user";
  var connectType = "follow";
  var posts = [],
      postsLength = 0,
      postsProgress = 0;
  
var video,
    togglePipButton,
    pipWindow;
  
  
  function loadVideo(url){
    var videoEl = makeVideo(url);
    videoEl.setAttribute("id", "mainVideo");
    menu({
      title:`Video Loaded!`,
      body:videoEl.outerHTML+`
      <button onclick="toggleVideo(this);" toggled="true" class="simple-round-xxlarge simple-button simple-theme-button">Pause</button>
      <button onclick="toggleButtons(this);" class="simple-round-xxlarge simple-button simple-theme-button">Buttons</button>
      <button id="videoPIP" class="simple-round-xxlarge simple-button simple-theme-button">Toggle PIP</button>
      `
    });
    video = document.getElementById("mainVideo");
    togglePipButton = document.getElementById("videoPIP");
    
    togglePipButton.addEventListener('click', async function(event) {
  console.log('Toggling Picture-in-Picture...');
  togglePipButton.disabled = true;
  try {
    if (video !== document.pictureInPictureElement) await video.requestPictureInPicture();
    else await document.exitPictureInPicture();
  }
  catch(error) { console.log(`> Argh! ${error}`); }
  finally { togglePipButton.disabled = false; }
});

video.addEventListener('enterpictureinpicture', function(event) {
  console.log(`> Window size is ${pipWindow.width}x${pipWindow.height}`); console.log('> Video entered Picture-in-Picture');
  pipWindow = event.pictureInPictureWindow;
  pipWindow.addEventListener('resize', onPipWindowResize);
});

video.addEventListener('leavepictureinpicture', function(event) {
  console.log('> Video left Picture-in-Picture');
  pipWindow.removeEventListener('resize', onPipWindowResize);
});
    
if ('pictureInPictureEnabled' in document) {
  setPipButton();
  video.addEventListener('loadedmetadata', setPipButton);
  video.addEventListener('emptied', setPipButton);
} else {
  togglePipButton.hidden = true;
}
    setTimeout(function(){ 
      video.play();
    }, 500);
  }
  
  function onPipWindowResize(event) {
    console.log(`> Window size changed to ${pipWindow.width}x${pipWindow.height}`);
  }

  function setPipButton() {
    togglePipButton.disabled = (video.readyState === 0) || !document.pictureInPictureEnabled || video.disablePictureInPicture;
  }
  
  
    function toggleVideo(El){
      if ( !El || !El.getAttribute("toggled") ) return
      var status = El.getAttribute("toggled") == "true";
      El.innerHTML = status ? "Play" : "Pause";
      El.setAttribute("toggled", status ? "false" : "true");
      status ? video.pause() : video.play();
    }
  
    function toggleButtons(El){
      video.controls = !video.controls;
    }
  
  async function MeownApi(input){
    var o=o||"";
    var r = await fetch(`/api/${input}`).then(l => l.json()).then(l => l.response)
    return r;
  }
  
  
  
  function displayNotification() {
  if (Notification.permission == 'granted') {
    navigator.serviceWorker.getRegistration().then(function(reg) {
      var options = {
        body: 'Here is a notification body!',
        icon: 'images/example.png',
        vibrate: [100, 50, 100],
        data: {
          dateOfArrival: Date.now(),
          primaryKey: 1
        }
      };
      reg.showNotification('Hello world!', options);
    });
  }
}
  
  
MeownApi(`${category}/${id}?includeposts=true`).then(async data => {
      var postList = document.getElementById("postList"),
          sendPost = document.getElementById("sendPost");
          posts = data.posts||[];
          postIndex = data.posts.length-1||0;
  
      if (sendPost&&id==me){sendPost.className = sendPost.className.replace("simple-hide", "");}
  
    while (postIndex > posts.length-max(posts.length+1,5)) {
      await addPost(data.posts[postIndex],[postIndex]);
      forEvery(postIndex,15,`loadAd();`);
      postIndex--;
    }
});
  
  function loadAd(){
          var advert = `Go check out my @orago 's page!`
    var adValues = {
      "title":"Sponsored Message",
      "body":`This is a advertisement ${advert}`,
      "likes":[],  
      "comments":[],       
      "index":3,           
      "id":"meown"
    }
    addPost(adValues,"Sponsored");
  }
    async function addPost(values,index){
      values = values || {}
      values.index=index;
      values.category == category;
      values.id=values.id||id;
      
      values.body = changeSymbols(values.body);
      
      var postTemplate = `
      <div class="simple-theme-l5 simple-theme-border simple-theme-text-1 simple-container simple-card simple-round simple-margin" id="post-3"><br>
        <img src="${await MeownApi(`/user/${values.id}/`).then(function(response){return response.avatar})}" alt="User_Avatar" class="simple-circle" style="width:90px;height:90px"> 
        <button class="simple-right simple-button simple-theme-button simple-round-xxlarge" style="" onclick="postMenu(${JSON.stringify(values).replace(/\"/g,"\'")})"><i class="fa fa-bars"></i></button>
        <span class="simple-right simple-opacity">
        #${index} 
        <i class="fa fa-heart"></i>:${values?.likes?.length?values.likes:0||0}</span>&nbsp;

        <br><b style="font-size: 2em;color:#e3c522;">&nbsp;${values.id||"unknown"}: </b>
        <a style="font-size: 1.5em;">${values.title}</a><br>
        <hr class="simple-clear">
        <p>${values.body}</p>
      </div>
      `;
        postList.innerHTML+=postTemplate;
    }
// Accordion
function postMenu(values){
